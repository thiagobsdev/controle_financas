package com.meusprojetos.controle_financas.entities;

public class User {

	private Long id;
	private String name;
	private String email;
	private String senha;
	
	public User() {
	}

	public User(Long id, String name, String email, String senha) {
		super();
		this.id = id;
		this.name = name;
		this.email = email;
		this.senha = senha;
	}
	
	
	
	
}
